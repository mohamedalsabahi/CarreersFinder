<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             Title="Register"
             x:Class="CarreersFinder.Views.Register">
    <ContentPage.Content>
        <StackLayout>



            <StackLayout Orientation="Vertical" VerticalOptions="CenterAndExpand">
            <Label Text="Register " HorizontalTextAlignment="Center"></Label>
                <Entry x:Name="etEmail" Placeholder="Email"></Entry>
                <Entry x:Name="etPassword" Placeholder="Password"></Entry>
            </StackLayout>



            <Button Text="Register"   Clicked="Button_Clicked_1" BackgroundColor="DarkOrange"></Button>
          
            
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>